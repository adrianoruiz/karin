  "event": [
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Armazenar token JWT automaticamente após login",
          "if (pm.response.json() && pm.response.json().access_token) {",
          "    pm.collectionVariables.set('jwt_token', pm.response.json().access_token);",
          "    console.log('Token JWT armazenado automaticamente!');",
          "}",
          "",
          "// Armazenar ID da triagem criada para usar em outros requests",
          "if (pm.response.json() && pm.response.json().id && pm.request.method === 'POST' && pm.request.url.path.includes('triage-records')) {",
          "    pm.collectionVariables.set('triage_id', pm.response.json().id);",
          "    console.log('ID da triagem armazenado: ' + pm.response.json().id);",
          "}"
        ]
      }
    }
